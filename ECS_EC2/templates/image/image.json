[
  {
    "name": "${container_name}",
    "essential": true,
    "image": "${app_image}",
    "memoryReservation": 128,
    "links": [],
    "entryPoint": [],
    "command": [],
    "environment": [],
   
    "volumesFrom": [],
    "portMappings": [
      {
        "containerPort": ${app_port},
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${awslogs_group}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "${container_name}-log-stream"
      }
    }
  }
]
